.contentpanel-root
  font-size: 12px;
  display: flex;
  flex-direction: column;
  h3
    margin: 3px;
  .contentpanel-bigimg
    width: 100%;
    height: 100px;
  .contentpanel-goods
    width: 100%;
    display: flex;
    flex-direction: row;
    list-style: none;
    margin: 0px;
    padding: 3px;
    overflow-x: auto;
    li
      margin-right: 10px;
      width: 33%;
      text-align: center;
    li > span
      display: flex;
      flex-direction: column;
    li > span > img
      height: 100px;
  .contentpanel-btn-more
    width:40%;
    height :25px;
    font-size :10px;



